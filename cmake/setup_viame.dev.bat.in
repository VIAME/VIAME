
REM This script sets up all paths required for standard VIAME Usage

SET VIAME_INSTALL=%~dp0

SET PATH=%VIAME_INSTALL%\bin;%VIAME_INSTALL%\Scripts;%PATH%

SET LD_LIBRARY_PATH=%VIAME_INSTALL%\bin;%PATH%

SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib\kwiver\plugins\algorithms;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib64\kwiver\plugins\algorithms;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib\kwiver\plugins\modules;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib64\kwiver\plugins\modules;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib\kwiver\plugins\processes;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib64\kwiver\plugins\processes;%KWIVER_PLUGIN_PATH%

SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib\viame\modules;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib64\viame\modules;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib\viame\processes;%KWIVER_PLUGIN_PATH%
SET KWIVER_PLUGIN_PATH=%VIAME_INSTALL%\lib64\viame\processes;%KWIVER_PLUGIN_PATH%

SET VIDTK_MODULE_PATH=%VIAME_INSTALL%\lib\modules
SET VG_PLUGIN_PATH=%VIAME_INSTALL%

SET SPROKIT_MODULE_PATH=%VIAME_INSTALL%\lib\sprokit;%SPROKIT_MODULE_PATH%
SET SPROKIT_PYTHON_MODULES=kwiver.sprokit.processes;kwiver.sprokit.schedulers
SET SPROKIT_PYTHON_MODULES=kwiver.sprokit.tests.processes;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=kwiver.arrows.core;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=kwiver.arrows.python;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=kwiver.sprokit.processes.pytorch;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=viame.arrows.core;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=viame.arrows.pytorch;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=viame.arrows.tensorflow;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=viame.arrows.smqtk;%SPROKIT_PYTHON_MODULES%
SET SPROKIT_PYTHON_MODULES=viame.processes;%SPROKIT_PYTHON_MODULES%

SET PYTHONPATH=%VIAME_INSTALL%\python;%PYTHONPATH%
SET PYTHONPATH=%VIAME_INSTALL%\lib\@VIAME_PYTHON_STRING@;%PYTHONPATH%
SET PYTHONPATH=%VIAME_INSTALL%\lib\@VIAME_PYTHON_STRING@\site-packages;%PYTHONPATH%

SET KWIVER_DEFAULT_LOG_LEVEL=debug
